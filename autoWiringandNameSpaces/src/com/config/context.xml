<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
xmlns:p="http://www.springframework.org/schema/p"
xmlns:c="http://www.springframework.org/schema/c"
 xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
 xsi:schemaLocation="
 http://www.springframework.org/schema/beans
 http://www.springframework.org/schema/beans/spring-beans-3.0.xsd
 ">
 <bean id="mob" class="com.pack.Mobile"  p:id="200" p:model="AAA" autowire="byType"/>
 <bean id="sm" class="com.pack.Sim" c:simId="500" c:provide="Airtel" primary="true"/>
 <bean id="si" class="com.pack.Sim" c:simId="5000" c:provide="Vodafone" />

 </beans>